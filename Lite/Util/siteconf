     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }
#endif
int
main ()
{
static int test_array [1 - 2 * !(((long) (sizeof (short int))) <= $ac_mid)];
test_array [0] = 0

  ;
  return 0;
